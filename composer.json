{
    "name": "loevgaard/linkmobility-bundle",
    "description": "Symfony bundle that integrates the Linkmobility PHP SDK into Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "knplabs/doctrine-behaviors": "^1.1",
        "loevgaard/linkmobility-php-sdk": "^1.0",
        "symfony/framework-bundle": "^3.0||^4.0",
        "symfony/validator": "^3.0||^4.0",
        "symfony/yaml": "^3.0||^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.5||^6.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\LinkmobilityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
